{
  "name": "smartstock-core",
  "version": "1.0.4",
  "scripts": {
    "ng": "ng",
    "box": "yarn install && yarn start",
    "start": "ng serve --disable-host-check=true --host=0.0.0.0 -c dev libs-mock",
    "build": "ng build -c production libs && node copy-workers.js",
    "build:lib": "npx ng build --configuration production libs",
    "publish": "npm run build && cd dist/libs && npm publish --ignore-script",
    "publish:beta": "npm run build && cd dist/libs && npm publish --tag beta",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^14.0.3",
    "@angular/cdk": "^13.0.1",
    "@angular/common": "^14.0.3",
    "@angular/compiler": "^14.0.3",
    "@angular/core": "^14.0.3",
    "@angular/forms": "^14.0.3",
    "@angular/material": "^13.0.1",
    "@angular/platform-browser": "^14.0.3",
    "@angular/platform-browser-dynamic": "^14.0.3",
    "@angular/router": "^14.0.3",
    "bfast": "^6.0.1",
    "browser-or-node": "^2.0.0",
    "comlink": "^4.3.1",
    "protractor": "7.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.0.3",
    "@angular/cli": "~14.0.3",
    "@angular/compiler-cli": "~14.0.3",
